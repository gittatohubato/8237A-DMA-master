add wave sim:/DMA/*
noforce sim:/DMA/clk
force -freeze sim:/DMA/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/DMA/IOW 0 0
force -freeze sim:/DMA/DMA_data_bus_in 5 0
# Invalid binary digit: 5.
# ** Error: (vsim-4011) Invalid force value: 5 0.
# 
force -freeze sim:/DMA/address_A_in 4'b0010 0
force -freeze sim:/DMA/HLDA 0 0
run
run
run
run
quit -sim
